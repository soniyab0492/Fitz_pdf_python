import fitz

def parse_pdf(pdf_path):

    with fitz.open(pdf_path) as my_pdf_file:

        for page_number in range(1, len(my_pdf_file) + 1):

            page = my_pdf_file[page_number - 1]

            for link in page.links():
                url = ''
                if "uri" in link:
                    url = link["uri"]

                match = re.search(u"https:[\/\w\-\.]+", url)
                
    
